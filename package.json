{
  "name": "@appello/common",
  "version": "0.1.20",
  "description": "Common usage",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib && tsc && tscpaths -p tsconfig.json -s ./src -o ./lib",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint --quiet src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "check-types": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run prettier && git add -A src",
    "postversion": "git push && git push --tags",
    "generateDocs": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts"
  },
  "keywords": [],
  "author": "Appello",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/appello-self/appello-kit.git",
    "directory": "common"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@appello/eslint-config": "0.1.7",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "axios": "0.26.1",
    "date-fns": "2.28.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "tscpaths": "0.0.9",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  }
}
